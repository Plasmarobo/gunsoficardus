
<div id="card_holder" class="card">
	<div class="card_left_margin"></div>
	<div class="card_center_content">
		<div class="card_name">
			<%= card.name %>
		</div>
		<div class="card_image" id="image">
		</div>
		<div class="card_text" id="text">
		</div>
	</div>
	<div class="card_right_content">
		<div class="symbol" id="edition">
			<img src="<%= card.edition.image %>"></img>
		</div>
		<div class="symbol" id="set">
			<img src="<%= card.cardset.image %>"></img>
		</div>
		<div class="card_right_spacer">
		</div>
		<div class="symbol" id="type">
			<img src="<%= card.type.image %>"></img>
		</div>
		<div class="card_right_spacer">
		</div>
		<div class="symbol" id="P">
		</div>
		<div class="symbol" id="E">
		</div>
		<div class="symbol" id="G">
		</div>
	</div>
</div>
